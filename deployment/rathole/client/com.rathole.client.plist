<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.rathole.client</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/rathole</string>
        <string>/usr/local/etc/rathole/client.toml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/rathole-client.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/rathole-client-error.log</string>
</dict>
</plist>
